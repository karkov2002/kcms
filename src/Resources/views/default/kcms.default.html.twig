{% extends '@KcmsBundle/default/__base.html.twig' %}

{% block title %}Welcome to Kcms{% endblock %}

{% block stylesheets %}
    {{ encore_entry_link_tags('kcms', null, 'kcms') }}
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css" integrity="sha512-iBBXm8fW90+nuLcSKlbmrPcLa0OT92xO1BIsZ+ywDWZCvqsWgccV3gFoRBv0z+8dLJgyAHIhR35VZc2oM/gI1w==" crossorigin="anonymous" />
{% endblock %}

{% block body %}
    <h1>Welcome to Kcms</h1>
    <p>This is the default (and empty) template. Be free to extends this template in your application</p>
    {% dump(kcms) %}
    {% for index,zone in kcms.zones %}
        <hr />
        Zone # {{ index }}
        <p>{{ zone|raw }}</p>
    {% endfor %}
    </ul>
{% endblock %}

{% block javascripts %}
    {{ kcms.js|raw }}
    {{ encore_entry_script_tags('kcms', null, 'kcms') }}
{% endblock %}
